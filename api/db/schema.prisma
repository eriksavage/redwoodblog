datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}
model Contact {
  id Int  @id @default(autoincrement())
  firstName String
  lastName String
  email String
  message String
  createdAt DateTime @default(now())
}

model Post {
  id Int  @id @default(autoincrement())
  title String
  body String
  createdAt DateTime @default(now())
}
